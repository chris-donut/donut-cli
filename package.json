{
  "name": "donut-cli",
  "version": "1.0.0",
  "description": "AI-powered crypto trading terminal with multi-agent orchestration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "donut": "./dist/index.js",
    "donut-mcp": "./dist/mcp-external/server.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npx ts-node src/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.js || true",
    "setup": "node dist/index.js setup"
  },
  "keywords": [
    "trading",
    "crypto",
    "cli",
    "ai",
    "agents",
    "backtest"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.77",
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@neynar/nodejs-sdk": "^3.121.0",
    "@polymarket/clob-client": "^5.2.1",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "dotenv": "^16.6.1",
    "hyperliquid": "^1.7.7",
    "ora": "^8.2.0",
    "pg": "^8.17.1",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "uuid": "^9.0.0",
    "viem": "^2.44.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  }
}
